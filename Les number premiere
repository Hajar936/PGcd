#include <stdio.h>

int main() {
    int n, i, j, estPremier;

    printf("Entrez un nombre entier : ");
    scanf("%d", &n);

    printf("Nombres premiers inférieurs à %d :\n", n);

    for(i = 2; i < n; i++) {
        estPremier = 1;
        for(j = 2; j <= i / 2; j++) {
            if(i % j == 0) {
                estPremier = 0;
                break;
            }
        }
        if(estPremier == 1) {
            printf("%d ", i);
        }
    }

    return 0;
